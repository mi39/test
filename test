<script>
function openURL() {
	var list =  document.links;  				// このhtml内のリンクのリスト
	var s = "";                  				// 出力用バッファ
	for( var i=0 ; i < list.length ; i++ ) {
		if("no" != list[i].name){
			open(list[i]);				// サイトを開く
			s += (i+1) + ':' + list[i] + '\n';
		}
	}
//	alert( s );						// デバッグ

}
</script>
</head>

<body onload="openURL();">
